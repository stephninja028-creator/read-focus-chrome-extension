{
  "manifest_version": 3,
  "name": "Read Focus",
  "version": "1.4",
  "description": "Let you stay on the current page and finish reading without distractions",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Read Focus",
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  }
}